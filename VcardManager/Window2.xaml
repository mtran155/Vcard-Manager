<Window x:Class="VcardManager.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VcardManager"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="{Binding myTitle}" Height="500" Width="800">
    <Grid Height="472" VerticalAlignment="Top">
        <ListBox x:Name="listBox" Margin="10,74,454,79"/>
        <ListBox x:Name="listBox1" Height="319" Margin="454,74,10,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Replace" Margin="366,114,366,0" VerticalAlignment="Top"/>
        <Button x:Name="button1" Content="Merge" Margin="366,208,366,0" VerticalAlignment="Top" />
        <Button x:Name="button2" Content="Cancel" Margin="366,300,366,0" VerticalAlignment="Top" IsCancel="True"/>
        <Label x:Name="label" Content="Card in Database" Margin="508,24,40,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="26.667"/>
        <Label x:Name="label1" Content="Card to Store" Margin="76,24,522,0" VerticalAlignment="Top" FontSize="26.667" FontWeight="Bold"/>
    </Grid>
</Window>
